card sus {
  spark card emacs
  spark card bash
  spark card xmonad
  spark card taffybar
}

card emacs {
  into ~

  .emacs
}

card bash {
  into ~
  outof bash

  .bashrc
  .bash_aliases
}

card xmonad {
  into ~/.xmonad
  outof xmonad

  xmonad.hs
}

card taffybar {
  into ~/.config/taffybar
  outof taffybar

  taffybar.hs
  taffybar.rc
}
